		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="DeleteVoicemailUserPreferencesRequest" type="tns:DeleteVoicemailUserPreferencesRequest"/>
   	<xsd:complexType name="DeleteVoicemailUserPreferencesRequest">
		<xsd:sequence>
			<xsd:element name="LoginRequest" type="tns:LoginRequest"/>
			<xsd:element name="mailboxNumber" type="xsd:string"/>
			<xsd:element name="domain" type="xsd:string"/>
			<xsd:element name="businessPartnerId" type="xsd:int"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="CreateVoicemailDialPlanRequest" type="tns:CreateVoicemailDialPlanRequest"/>
   	<xsd:complexType name="CreateVoicemailDialPlanRequest">
		<xsd:sequence>
			<xsd:element name="LoginRequest" type="tns:LoginRequest"/>
			<xsd:element name="mailboxNumber" type="xsd:string"/>
			<xsd:element name="businessPartnerId" type="xsd:int"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="DeleteVoicemailDialPlanRequest" type="tns:DeleteVoicemailDialPlanRequest"/>
   	<xsd:complexType name="DeleteVoicemailDialPlanRequest">
		<xsd:sequence>
			<xsd:element name="LoginRequest" type="tns:LoginRequest"/>
